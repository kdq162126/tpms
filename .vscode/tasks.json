{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "--include-directory=${workspaceFolder}/src/component/at_modem",
                "--include-directory=${workspaceFolder}/src/component/ec200u",
                "--include-directory=${workspaceFolder}/src/service/timeout_clock",
                "--include-directory=${workspaceFolder}/src/utils/string_util",
                "--include-directory=${workspaceFolder}/src/utils/printf",
                "--include-directory=${workspaceFolder}/src/service/segment",
                "-lm"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}